
public class IntList {
	
	private int[] data; 
	private int elementCount;
	
	public IntList(){
		elementCount = 0;
		setData(new int[2000000]);
	}
	
	public void add(int value){
		data[elementCount] = value;
		elementCount ++;
	}
	
	public int get(int index){
		return 0;
	}
	
	public int size(){
		return 0;
	}

	public int[] getData() {
		return data;
	}

	public void setData(int[] data) {
		this.data = data;
	}
}
