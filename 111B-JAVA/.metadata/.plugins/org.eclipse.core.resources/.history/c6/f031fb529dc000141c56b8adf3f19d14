
public class MyCircle {
	
	double x;
	double y;
	double radius;
	
	void setX(double x){
		this.x = x;
	}
	
	double getX(){
		return this.x;
	}
	
	void setY(double y){
		this.y = y;
	}
	
	double getY(){
		return this.y;
	}
	
	void setRadius(double radius){
		this.radius = radius; 
	}
	
	double getRadius(){
		return this.radius;
	}
	
	double calculateArea(){
		return radius * radius * Math.PI;
	}
	
	double getArea(){
		return calculateArea();
	}
	
	boolean doesOverlap(MyCircle otherCircle){
		double x2;
		double y2;
		
		x2 = otherCircle.x;
		y2 = otherCircle.y;
		
		return true;
	}
}
