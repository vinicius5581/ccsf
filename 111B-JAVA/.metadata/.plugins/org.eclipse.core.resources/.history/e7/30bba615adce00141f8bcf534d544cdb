
public class IntList2 {
	
	
	private int elementCount;
	private ArrayList<Integer> data;
	
	public IntList(){
		elementCount = 0;
		data = new ArrayList<Integer>();
	}
	
	public void add(int value){
		data.add(value);
	}
	
	public int get(int index){
		return data[index];
	}
	
	public int size(){
		return elementCount;
	}

	public int[] getData() {
		return data;
	}

	public void setData(int[] data) {
		this.data = data;
	}

}
